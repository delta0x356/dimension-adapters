{
  "ts-node": {
    "compilerOptions": {
      "module": "ESNext"
    },
    "esm": true
  },
  "compilerOptions": {
    "target": "es2019",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "preserve",
    "allowJs": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "lib": [
      "ES2019",
      "DOM"
    ]
  },
  "include": [
    "fees/*",
    "dexs/*",
    "adapters/*",
    "aggregators/*",
    "options/*",
    "incentives/*",
    "fees/*",
    "protocols/*",
    "fees/gmx-v2/gmx-v2.ts",
    "fees/tigris/index.ts",
    "fees/xoxno/xoxno.ts"
  ],
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}